   <Constraints>
      <Constraint
         Name="CheckSubRequired"
         ConditionType="RejectWhen"
         ErrorAttribute=""
         >
         <Condition><![CDATA[ SUBJECT  IS  NULL  AND  NOTIF_TYP  IS  NOT  NULL  AND  NOTIF_TYP = CVL_NOTIF_TYPImpl.EMAIL]]></Condition>
         <ErrorMessage><![CDATA[%c:V203,v:[b:R_EXT_EMAIL_LTRCNF/SUBJECT],v:[b:R_EXT_EMAIL_LTRCNF/NOTIF_TYP]%]]></ErrorMessage>
      </Constraint>
      <Constraint
         Name="CheckSubRequired2"
         ConditionType="RejectWhen"
         ErrorAttribute=""
         >
         <Condition><![CDATA[ SUBJECT  IS  NOT  NULL  AND  NOTIF_TYP  IS  NOT  NULL  AND  NOTIF_TYP  = CVL_NOTIF_TYPImpl.LETTER]]></Condition>
         <ErrorMessage><![CDATA[%c:V204,v:[b:R_EXT_EMAIL_LTRCNF/SUBJECT],v:[b:R_EXT_EMAIL_LTRCNF/NOTIF_TYP]%]]></ErrorMessage>
      </Constraint>
      <Constraint
         Name="CheckCommentIntroText"
         ConditionType="RejectWhen"
         ErrorAttribute=""
         >
         <Condition><![CDATA[ CMNT_INTRO_TXT  IS  NULL  AND  CMNT_INCL  =  True /* Yes */  ]]></Condition>
         <ErrorMessage><![CDATA[%c:V206,v:[b:R_EXT_EMAIL_LTRCNF/CMNT_INTRO_TXT],v:[b:R_EXT_EMAIL_LTRCNF/CMNT_INCL]%]]></ErrorMessage>
      </Constraint>
      <Constraint
         Name="CheckCommentsInclText"
         ConditionType="RejectWhen"
         ErrorAttribute=""
         >
         <Condition><![CDATA[ CMNT_INCL  =   True /* Yes */    AND  EMAIL_LTR_TYP   NOT In ('CERAPP', 'CERREJ', 'CERAPL', 'CHGAPP' , 'CHGREJ') ]]></Condition>
         <ErrorMessage><![CDATA[%c:V205%]]></ErrorMessage>
      </Constraint>
      <Constraint
         Name="CheckCommentIntroText2"
         ConditionType="RejectWhen"
         ErrorAttribute=""
         >
        <Condition><![CDATA[AMSStringUtil.strIsEmpty(getCMNT_INTRO_TXT()) = false  AND  CMNT_INCL  =   False /* No */  ]]></Condition>
        <ErrorMessage><![CDATA[%c:V207,v:[b:R_EXT_EMAIL_LTRCNF/CMNT_INTRO_TXT],v:[b:R_EXT_EMAIL_LTRCNF/CMNT_INCL]%]]></ErrorMessage>
      </Constraint>
      <Constraint
         Name="CheckUpdVendNotification"
         ConditionType="RejectWhen"
         ErrorAttribute=""
         >
         <Condition><![CDATA[ UPD_VEND_NOTIF_TBL  =  True /* Yes */     AND  EMAIL_LTR_TYP   NOT In ('VENNOT', 'SOLAMD', 'SOLCAN', 'NOTAWD', 'NOINAW', 'VDOVBD', 'VDUNBD') ]]></Condition>
         <ErrorMessage><![CDATA[%c:V208%]]></ErrorMessage>
      </Constraint>
      <Constraint
         Name="CheckSMTPServer"
         ConditionType="RejectWhen"
         ErrorAttribute=""
         >
         <Condition><![CDATA[ SMTP_SRV_NM  IS  NULL  AND  NOTIF_TYP  = CVL_NOTIF_TYPImpl.EMAIL]]></Condition>
         <ErrorMessage><![CDATA[%c:V209,v:[b:R_EXT_EMAIL_LTRCNF/SMTP_SRV_NM]%]]></ErrorMessage>
      </Constraint>
      <Constraint
         Name="CheckAutoEmailAddress"
         ConditionType="RejectWhen"
         ErrorAttribute=""
         >
         <Condition><![CDATA[ AUTO_SEND_EMAIL_AD  IS  NULL  AND  NOTIF_TYP  = CVL_NOTIF_TYPImpl.EMAIL]]></Condition>
         <ErrorMessage><![CDATA[%c:V210,v:[b:R_EXT_EMAIL_LTRCNF/AUTO_SEND_EMAIL_AD]%]]></ErrorMessage>
      </Constraint>
   </Constraints>
