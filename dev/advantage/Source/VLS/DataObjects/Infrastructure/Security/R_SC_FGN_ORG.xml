	<Constraints>
		<Constraint
			Name="wildCrdUserIdEntry"
			ConditionType="RejectWhen"
			ErrorAttribute=""
			>
			<Condition><![CDATA[(Inserting  OR  Updating)  AND getSecurityRole() != NULL AND getSecurityRole(). getFGN_ORG_RSTR() = CVL_FGN_ORG_CNFGImpl.USER_ID_REQUIRED  AND getUSER_ID().equals('ALL') = True]]></Condition>
			<ErrorMessage><![CDATA[%c:Q0119%]]></ErrorMessage>
		</Constraint>
		<Constraint
			Name="nonWildCrdUserIdEntry"
			ConditionType="RejectWhen"
			ErrorAttribute=""
			>
			<Condition><![CDATA[(Inserting  OR  Updating)  AND getSecurityRole() !=  NULL AND (getSecurityRole(). getFGN_ORG_RSTR() = CVL_FGN_ORG_CNFGImpl.NONE OR getSecurityRole(). getFGN_ORG_RSTR() = CVL_FGN_ORG_CNFGImpl.HOME_DEPT_REQUIRED OR (getSecurityRole(). getFGN_ORG_RSTR() = CVL_FGN_ORG_CNFGImpl.USERID_OR_HOMEDEPT_REQUIRED AND getHOME_DEPT_CD().equals('ALL') = False))  AND getUSER_ID().equals('ALL') = False]]></Condition>
			<ErrorMessage><![CDATA[%c:Q0120%]]></ErrorMessage>
		</Constraint>
		<Constraint
			Name="validUserId"
			ConditionType="RejectWhen"
			ErrorAttribute="USER_ID"
			>
			<Condition><![CDATA[(Inserting  OR  Updating)  AND getSecurityRole() !=  NULL AND getSecurityRole(). getFGN_ORG_RSTR() = CVL_FGN_ORG_CNFGImpl.USER_ID_REQUIRED  AND getUSER_ID().equals('ALL') = False AND (getR_SC_USER_ROLE_LNK() = null)]]></Condition>
			<ErrorMessage><![CDATA[%c:Q0024,v:[b:R_SC_FGN_ORG/USER_ID],v:[b:R_SC_FGN_ORG/SEC_ROLE_ID]%]]></ErrorMessage>
		</Constraint>
		<Constraint
			Name="isHomeDeptAllValid"
			ConditionType="RejectWhen"
			ErrorAttribute=""
			>
			<Condition><![CDATA[(Inserting  OR  Updating) AND (AMSStringUtil.strEqual(getHOME_DEPT_CD(),AMSCommonConstants.ALL) = True OR AMSStringUtil.strEqual(getUSER_ID(),AMSCommonConstants.ALL) = False ) AND getSecurityRole() !=  NULL AND getSecurityRole().getFGN_ORG_RSTR() = CVL_FGN_ORG_CNFGImpl.HOME_DEPT_REQUIRED]]></Condition>
			<ErrorMessage><![CDATA[%c:Q0160%]]></ErrorMessage>
		</Constraint>
		<Constraint
			Name="isHomeDeptNotAllValid"
			ConditionType="RejectWhen"
			ErrorAttribute=""
			>
			<Condition><![CDATA[(Inserting  OR  Updating) AND AMSStringUtil.strEqual(getHOME_DEPT_CD(),AMSCommonConstants.ALL) = False AND getSecurityRole() !=  NULL AND ( getSecurityRole().getFGN_ORG_RSTR() = CVL_FGN_ORG_CNFGImpl.USER_ID_REQUIRED OR getSecurityRole().getFGN_ORG_RSTR() = CVL_FGN_ORG_CNFGImpl.NONE )]]></Condition>
			<ErrorMessage><![CDATA[%c:Q0161%]]></ErrorMessage>
		</Constraint>
		<Constraint
			Name="isDeptAndUserIDAllValid"
			ConditionType="RejectWhen"
			ErrorAttribute=""
			>
			<Condition><![CDATA[(Inserting  OR  Updating) AND AMSStringUtil.strEqual(getHOME_DEPT_CD(),AMSCommonConstants.ALL) = True AND AMSStringUtil.strEqual(getUSER_ID(),AMSCommonConstants.ALL) = True AND getSecurityRole() !=  NULL AND getSecurityRole().getFGN_ORG_RSTR() = CVL_FGN_ORG_CNFGImpl.USERID_OR_HOMEDEPT_REQUIRED]]></Condition>
			<ErrorMessage><![CDATA[%c:Q0162%]]></ErrorMessage>
		</Constraint>
	</Constraints>
