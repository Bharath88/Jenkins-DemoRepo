			<ActionToPerform><![CDATA[validateSecurityRoles()]]></ActionToPerform>
			<Condition><![CDATA[(  (  DOC_ACTN_CD  = AMSCommonConstants.DOC_ACTN_VALIDATE )  OR  (  DOC_ACTN_CD  = AMSCommonConstants.DOC_ACTN_SUBMIT )  ) ]]></Condition>
