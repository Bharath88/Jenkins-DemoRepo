	<Constraints>
		<Constraint
			Name="createFrgOrgRecByUserId"
			ConditionType="RejectWhen"
			ErrorAttribute=""
			>
			<Condition><![CDATA[(Inserting OR Updating)   AND getR_SC_SEC_ROLE() !=  NULL AND  getR_SC_SEC_ROLE().getFGN_ORG_RSTR() = CVL_FGN_ORG_CNFGImpl.USER_ID_REQUIRED]]></Condition>
