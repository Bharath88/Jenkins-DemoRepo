	<Constraints>
		<Constraint
			Name="DOC_ROW_SEC"
			ConditionType="RejectWhen"
			ErrorAttribute="ROW_SEC_FL"
			>
			<ErrorMessage><![CDATA[Row level security cannot be enabled for documents]]></ErrorMessage>
			<Condition><![CDATA[ (  Inserting  OR  Updating  )  AND  (  (  RSRC_TYP_IND  =  'D' /* Document */   )  AND  (  ROW_SEC_FL  =  True /* Yes */   )  ) ]]></Condition>
		</Constraint>
		<Constraint
			Name="ORG_SEC_TYP"
			ConditionType="RejectWhen"
			>
			<ErrorMessage><![CDATA[The organization indicator has to be Both for resource type Document.]]></ErrorMessage>
			<Condition><![CDATA[(  RSRC_TYP_IND  =  'D' /* Document */   )  AND  (  ORG_LVL_CD  !=  'B'  /* Both */   )]]></Condition>
		</Constraint>
		<Constraint
			Name="ValidatePageSecFl"
			ConditionType="RejectWhen"
			ErrorAttribute="PG_SEC_FL"
			>
			<Condition><![CDATA[(   ( PG_SEC_FL  =  True /* Yes */ )  AND ( ( RSRC_TYP_IND  = 'D' /* Document */ )  OR ( RSRC_TYP_IND  = 'Q' /* Query */ ) OR ( RSRC_TYP_IND  = 'T' /* Reference Table */ ) )  ) ]]></Condition>
			<ErrorMessage><![CDATA[%c:A4003%]]></ErrorMessage>
		</Constraint>
		<Constraint
			Name="ValidateFieldSecFl"
			ConditionType="RejectWhen"
			ErrorAttribute="UI_FLD_SEC_FL"
			>
			<Condition><![CDATA[ (  UI_FLD_SEC_FL  =  True /* Yes */   )  AND  (  RSRC_TYP_IND  <>  'P' /* Page */   ) ]]></Condition>
			<ErrorMessage><![CDATA[%c:A4004%]]></ErrorMessage>
		</Constraint>
	</Constraints>
